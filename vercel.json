{
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/"
    },
    {
      "src": "/docs/swagger-ui/(.*)",
      "dest": "/public/swagger-ui/$1"
    },
    {
      "src": "/api-docs",
      "dest": "src/index.ts"
    }
  ]
}
